{
    "name": "__MSG_appName__",
    "description": "__MSG_shortDesc__",
    "default_locale": "en",
    "version": "1.0.7",
    "manifest_version": 3,
    "icons": {
        "260": "icon-default.png"
    },
    "permissions": [
        "sidePanel"
    ],
    "action": {
        "default_icon": {
            "260": "icon-default.png"
        },
        "default_title": "AI Math"
    },
    "side_panel": {
        "default_path": "sidepanel.html"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "*img/icons/*",
                "*scripts/vendors/*",
                "*scripts/common/*"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "scripts/background.js"
    }
}